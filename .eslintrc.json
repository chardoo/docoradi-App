{
    "parserOptions": {
        "parser":"babel-eslint",
        "ecmaVersion": 8,
        "sourceType": "module"
       
    },
    "extends": ["airbnb-base"],
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "linebreak-style": ["error", "unix"],
        "no-console": "off",
        "no-redeclare": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "no-mixed-spaces-and-tabs": "off",
        "no-unreachable": "warn",
        
    }
}
